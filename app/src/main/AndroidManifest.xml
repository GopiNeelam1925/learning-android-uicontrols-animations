<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cencol.kevinma_comp304lab3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cencol.kevinma_comp304Lab3_Ex1.CanvasPaint"
            android:label="@string/cpa_label"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name="com.cencol.kevinma_comp304Lab3_Ex2.FrameAnimation"
            android:label="@string/ex2_title_label"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name="com.cencol.kevinma_comp304Lab3_Ex3.TweenAnimation"
            android:label="@string/ex3_title_label"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>